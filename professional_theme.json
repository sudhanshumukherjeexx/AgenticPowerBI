{
  "name": "Corporate Executive Dashboard Theme",
  "dataColors": [
    "#2E5BBA", 
    "#1B9E77", 
    "#D32F2F", 
    "#FBC02D", 
    "#388E3C", 
    "#FFA726", 
    "#7B1FA2", 
    "#0288D1", 
    "#F57C00", 
    "#455A64", 
    "#C2185B", 
    "#0097A7"
  ],
  "background": "#F5F7FA",
  "foreground": "#222222",
  "tableAccent": "#2E5BBA",
  "visualStyles": {
    "*": {
      "*": {
        "background": [{"color": {"solid": {"color": "#FFFFFF"}}}],
        "border": [{"color": {"solid": {"color": "#B0BEC5"}}}]
      }
    },
    "card": {
      "*": {
        "background": [{"color": {"solid": {"color": "#FFFFFF"}}}],
        "border": [{"color": {"solid": {"color": "#B0BEC5"}}}],
        "dropShadow": [{"show": true, "color": {"solid": {"color": "#00000020"}}}]
      }
    },
    "tableEx": {
      "*": {
        "background": [{"color": {"solid": {"color": "#FFFFFF"}}}],
        "border": [{"color": {"solid": {"color": "#B0BEC5"}}}]
      }
    },
    "slicer": {
      "*": {
        "background": [{"color": {"solid": {"color": "#FFFFFF"}}}],
        "border": [{"color": {"solid": {"color": "#B0BEC5"}}}]
      }
    }
  },
  "textClasses": {
    "callout": {
      "fontSize": 45,
      "fontFace": "Segoe UI Light",
      "color": "#2E5BBA"
    },
    "title": {
      "fontSize": 20,
      "fontFace": "Segoe UI Semibold",
      "color": "#2E5BBA"
    },
    "header": {
      "fontSize": 16,
      "fontFace": "Segoe UI Semibold", 
      "color": "#222222"
    },
    "label": {
      "fontSize": 13,
      "fontFace": "Segoe UI",
      "color": "#455A64"
    }
  }
}